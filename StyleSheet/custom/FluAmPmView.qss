#scrollDownBtn {
    background: rgba(243, 145, 199, 0.95);
    border: 1px solid transparent;
    border-radius: 4px;
}

#scrollUpBtn {
    background: rgba(243, 145, 199, 0.95);
    border: 1px solid transparent;
    border-radius: 4px;
}