FluCodeBox {
    background-color: rgba(243, 145, 199, 0.95);
    border: 1px solid rgb(255, 172, 220);
    font-size: 14px;
    border-radius: 4px;
    selection-background-color: rgba(243, 145, 199, 0.8);
    selection-color: rgb(255, 255, 255);
    color: rgb(73, 66, 71);
}

FluCodeBox[transparent=true] {
    background-color: transparent;
    font-size: 14px;
    border: none;
}